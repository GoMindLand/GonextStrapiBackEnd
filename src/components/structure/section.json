{
  "collectionName": "components_structure_sections",
  "info": {
    "displayName": "section",
    "description": ""
  },
  "options": {},
  "attributes": {
    "BackgroundImage": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "row": {
      "type": "component",
      "repeatable": true,
      "component": "structure.row"
    },
    "couleur": {
      "type": "customField",
      "regex": "^#([A-Fa-f0-9]{6}|[A-Fa-f0-9]{3})$",
      "customField": "plugin::color-picker.color"
    },
    "Hauteur": {
      "type": "integer",
      "default": 50,
      "max": 100,
      "min": 0
    }
  }
}
